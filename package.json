{
  "name": "@fvilers/heroicons-svelte",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint . && prettier --check .",
    "format": "prettier --write .",
    "prepare": "husky",
    "generate": "tsx ./generate",
    "prebuild": "npm run generate"
  },
  "engines": {
    "node": "^20"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "exports": {
    "./16/solid": {
      "types": "./dist/16/solid/index.d.ts",
      "svelte": "./dist/16/solid/index.js"
    },
    "./20/solid": {
      "types": "./dist/20/solid/index.d.ts",
      "svelte": "./dist/20/solid/index.js"
    },
    "./24/outline": {
      "types": "./dist/24/outline/index.d.ts",
      "svelte": "./dist/24/outline/index.js"
    },
    "./24/solid": {
      "types": "./dist/24/solid/index.d.ts",
      "svelte": "./dist/24/solid/index.js"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@types/eslint": "^9.6.0",
    "@types/node": "^20.17.0",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0",
    "globals": "^15.0.0",
    "husky": "^9.1.6",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.6",
    "publint": "^0.2.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tsx": "^4.19.1",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.0.11"
  }
}
